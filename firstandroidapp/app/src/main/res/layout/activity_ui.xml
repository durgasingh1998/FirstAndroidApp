<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"

    tools:context=".UI">

    <Button
        android:id="@+id/button8"
        android:layout_width="165dp"
        android:layout_height="113dp"
        android:background="#1E7E73"
        android:drawableRight="@drawable/ic_baseline_create_24"
        android:drawablePadding="20dp"
        android:text="Create Resume"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:backgroundTint="@color/teal_700"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="215dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="165dp"
        android:layout_height="113dp"
        android:background="@color/teal_700"
        android:drawableRight="@drawable/ic_baseline_menu_book_24"
        android:drawablePadding="20dp"
        android:text="View Resume"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:backgroundTint="@color/teal_700"

        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="215dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="165dp"
        android:layout_height="113dp"
        android:drawableRight="@drawable/ic_baseline_share_24"
        android:drawablePadding="20dp"
        android:text="Share"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:backgroundTint="@color/teal_700"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="417dp" />
        />

    <Button
        android:id="@+id/button11"
        android:layout_width="165dp"
        android:layout_height="113dp"
        android:drawableRight="@drawable/ic_baseline_save_alt_24"
        android:drawablePadding="20dp"
        android:text="Download"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:backgroundTint="@color/teal_700"
        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="417dp" />
        />
</androidx.constraintlayout.widget.ConstraintLayout>